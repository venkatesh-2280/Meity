<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        


        android:id="@+id/rp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#333333">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    >
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/colorPrimaryDark2">
                        <ImageView
                            android:layout_width="30sp"
                            android:layout_height="30sp"
                            android:id="@+id/del5"
                            android:visibility="invisible"
                            android:background="@drawable/del"

                            android:layout_alignParentEnd="true"
                            android:layout_centerVertical="true"
                            android:layout_margin="5sp"
                            android:layout_alignParentRight="true" />
                        <ImageView
                            android:layout_width="30sp"
                            android:layout_height="30sp"
                            android:id="@+id/ed"
                            android:visibility="invisible"

                            android:background="@drawable/ed"
                            android:layout_toLeftOf="@+id/del5"

                            android:layout_centerVertical="true"
                            android:layout_margin="5sp"
                            />


                    </RelativeLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="11sp"
                            android:text="Year:"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:textStyle="bold"
                            android:textColor="#ffffff"
                            android:padding="5sp"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="11sp"
                            android:textAlignment="viewEnd"
                            tools:ignore="RtlCompat"
                            android:id="@+id/t1"
                            android:layout_weight="1"
                            android:text=""
                            android:layout_gravity="center"
                            android:textColor="#ffffff"
                            android:padding="5sp"
                            />
                    </LinearLayout>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="11sp"
                            android:text="Season:"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:textStyle="bold"
                            android:textColor="#ffffff"
                            android:padding="5sp"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="11sp"
                            android:id="@+id/t2"
                            android:textAlignment="viewEnd"
                            tools:ignore="RtlCompat"
                            android:layout_weight="1"
                            android:text=""
                            android:layout_gravity="center"
                            android:textColor="#ffffff"
                            android:padding="5sp"/>

                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="11sp"
                            android:text="Crop Classification:"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:textStyle="bold"
                            android:textColor="#ffffff"
                            android:padding="5sp"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="11sp"
                            android:id="@+id/t3"
                            android:textAlignment="viewEnd"
                            tools:ignore="RtlCompat"
                            android:layout_weight="1"
                            android:text=""
                            android:layout_gravity="center"
                            android:textColor="#ffffff"
                            android:padding="5sp"/>

                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="11sp"
                            android:text="Crop Name:"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:textStyle="bold"
                            android:textColor="#ffffff"
                            android:padding="5sp"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="11sp"
                            android:id="@+id/t4"
                            android:textAlignment="viewEnd"
                            tools:ignore="RtlCompat"
                            android:layout_weight="1"
                            android:text=""
                            android:layout_gravity="center"
                            android:textColor="#ffffff"
                            android:padding="5sp"/>

                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="11sp"
                            android:text="Crop Variety:"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:textStyle="bold"
                            android:textColor="#ffffff"
                            android:padding="5sp"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="11sp"
                            android:id="@+id/t5"
                            android:textAlignment="viewEnd"
                            tools:ignore="RtlCompat"
                            android:layout_weight="1"
                            android:text=""
                            android:layout_gravity="center"
                            android:textColor="#ffffff"
                            android:padding="5sp"/>

                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="11sp"
                            android:text="Seed Name:"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:textStyle="bold"
                            android:textColor="#ffffff"
                            android:padding="5sp"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="11sp"
                            android:id="@+id/t6"
                            android:textAlignment="viewEnd"
                            tools:ignore="RtlCompat"
                            android:layout_weight="1"
                            android:text=""
                            android:layout_gravity="center"
                            android:textColor="#ffffff"
                            android:padding="5sp"/>

                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone"

                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="11sp"
                            android:text="Seed Qty(Kg):"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:textStyle="bold"
                            android:textColor="#ffffff"
                            android:padding="5sp"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="11sp"
                            android:id="@+id/t7"
                            android:textAlignment="viewEnd"
                            tools:ignore="RtlCompat"
                            android:layout_weight="1"
                            android:text=""
                            android:layout_gravity="center"
                            android:textColor="#ffffff"
                            android:padding="5sp"/>

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="11sp"
                            android:text="Actual Production:"
                            android:layout_weight="1"

                            android:layout_gravity="center"
                            android:textStyle="bold"
                            android:textColor="#ffffff"
                            android:padding="5sp"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="11sp"
                            android:id="@+id/t8"
                            android:textAlignment="viewEnd"
                            tools:ignore="RtlCompat"
                            android:layout_weight="1"
                            android:text=""
                            android:layout_gravity="center"
                            android:textColor="#ffffff"
                            android:padding="5sp"/>

                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="11sp"
                            android:text="Produce Available For Sale:"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:textStyle="bold"
                            android:textColor="#ffffff"
                            android:padding="5sp"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="11sp"
                            android:id="@+id/t9"
                            android:textAlignment="viewEnd"
                            tools:ignore="RtlCompat"
                            android:layout_weight="1"
                            android:text=""
                            android:layout_gravity="center"
                            android:textColor="#ffffff"
                            android:padding="5sp"/>

                    </LinearLayout>

                </LinearLayout>




            </LinearLayout>
        </RelativeLayout>

    </RelativeLayout>

</RelativeLayout>